package net.amicom;

public class Student {
	private String name;
	private int age;
	private int grade;
	private int sudentNum;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public int getGrade() {
		return grade;
	}

	public void setGrade(int grade) {
		this.grade = grade;
	}

	public int getSudentNum() {
		return sudentNum;
	}

	public void setSudentNum(int sudentNum) {
		this.sudentNum = sudentNum;
	}

}
